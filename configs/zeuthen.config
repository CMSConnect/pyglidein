[Mode]
debug = True

[Glidein]
address = http://glidein-simprod.icecube.wisc.edu:11001/jsonrpc
delay = 600
loc = /afs/ifh.de/group/amanda/icecube/icegrid/pyglidein
# tarball = glidein.tar.gz
executable = glidein_start.sh 

[Cluster]
user = iceprod
os = RHEL6
scheduler = UGE
submit_command = qsub
max_total_jobs = 80
limit_per_submit = 150
cvmfs = True
mem_per_core = 2700
walltime_hrs = 48
gpu_only = True
